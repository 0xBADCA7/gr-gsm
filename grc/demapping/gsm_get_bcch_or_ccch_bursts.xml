<?xml version="1.0"?>
<block>
  <name>Demapper for BCCH and CCCH</name>
  <key>gsm_get_bcch_or_ccch_bursts</key>
  <import>import grgsm</import>
  <make>grgsm.get_bcch_or_ccch_bursts($d_fn51_start)</make>
  <param>
    <name>d_fn51_start</name>
    <key>d_fn51_start</key>
    <value>2</value>
    <type>int</type>
  </param>

  <sink>
    <name>bursts</name>
    <type>message</type>
  </sink>
  <source>
    <name>bursts</name>
    <type>message</type>
  </source>
</block>
